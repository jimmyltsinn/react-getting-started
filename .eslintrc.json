{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": false,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "semi": 1,
    "no-unused-vars": 1
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [ "react" ]
}
